{
  "name": "neomerx/cors-illuminate",
  "description": "CORS (Cross-Origin Resource Sharing) support for Laravel and Lumen",
  "keywords": [
    "Cross-Origin Resource Sharing",
    "Cross Origin Resource Sharing",
    "CORS",
    "Laravel",
    "Lumen",
    "neomerx"
  ],
  "homepage": "https://github.com/neomerx/cors-illuminate",
  "support": {
    "issues": "https://github.com/neomerx/cors-illuminate/issues"
  },
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "neomerx",
      "email": "info@neomerx.com"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "psr/http-message": "^1.0",
    "psr/log": "^1.0",
    "illuminate/http": "^5.1",
    "neomerx/cors-psr7": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.6",
    "mockery/mockery": "^0.9.4",
    "scrutinizer/ocular": "^1.1",
    "squizlabs/php_codesniffer": "^2.0"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Neomerx\\CorsIlluminate\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Neomerx\\Tests\\CorsIlluminate\\": "tests/"
    }
  },
  "scripts": {
    "test": "composer phpunit && composer phpcs",
    "phpcs": "./vendor/bin/phpcs -p -s --standard=PSR2 ./src ./tests",
    "phpunit": "phpunit",
    "hhvm": "hhvm ./vendor/phpunit/phpunit/phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
